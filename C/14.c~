#include<stdio.h>

int main(void)
{
	int a, b, c, m[12];

	printf("输入年月日，用空格隔开\n");
	scanf("%d%d%d",&a, &b, &c);
	
	if (a % 4 == 0 && a % 100 != 0 || a % 400 == 0)
	{
		m[] = {31,28,31,30,31,30,31,31,30,31,30,31}
	}
	else
	{
		
	}
	return 0;
}
