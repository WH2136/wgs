KERNELDIR	Makefile	/^KERNELDIR := \/usr\/src\/linux-headers-3.16.0-4-amd64 $/;"	m
interface	myirq.c	/^static char *interface;$/;"	v	file:
irq	myirq.c	/^static int irq;$/;"	v	file:
myinterrupt	myirq.c	/^static irqreturn_t myinterrupt(int irq, void *dev_id)$/;"	f	file:
myirqtest_exit	myirq.c	/^module_exit(myirqtest_exit);$/;"	v
myirqtest_exit	myirq.c	/^static void __exit myirqtest_exit(void)$/;"	f	file:
myirqtest_init	myirq.c	/^module_init(myirqtest_init);$/;"	v
myirqtest_init	myirq.c	/^static int __init myirqtest_init(void)$/;"	f	file:
obj-m	Makefile	/^obj-m := myirq.o$/;"	m
